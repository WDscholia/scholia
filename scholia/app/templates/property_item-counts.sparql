#defaultView:BubbleChart

PREFIX target: <http://www.wikidata.org/prop/direct/{{ p }}>

SELECT ?count ?s ?sLabel 
WITH {
  SELECT (COUNT(?o) AS ?count) ?s WHERE {
    ?s target: ?o .
  }
  GROUP BY ?s
  ORDER BY DESC(?count)
  LIMIT 100
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language  "[AUTO_LANGUAGE],en" . }
}
ORDER BY DESC(?count)
