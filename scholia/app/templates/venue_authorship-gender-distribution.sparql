# title: Authorship gender distribution for a specific venue
SELECT ?count ?gender ?genderLabel 
WITH {
  SELECT (COUNT(?author) AS ?count) ?gender WHERE {
    ?work wdt:P1433 wd:{{ q }} .
    ?work wdt:P50 ?author .
    ?author wdt:P21 ?gender .
  } 
  GROUP BY ?gender
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language "{{ g.lang_code }},[AUTO_LANGUAGE],en" . } 
} 
 ORDER BY DESC(?count)
